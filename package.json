  "devDependencies": {
    "@types/cors": "^2.8.5",
    "@types/dotenv": "^6.1.1",
    "@types/helmet": "^0.0.43",
    "@types/jsonwebtoken": "^8.3.2",
    "@types/morgan": "^1.7.36",
    "@types/node": "^12.6.8",
    "@types/nodemailer": "^6.2.1",
    "@types/passport": "^1.0.0",
    "@types/passport-jwt": "^3.0.1",
    "babel-runtime": "^6.26.0",
    "nodemon": "^1.19.1",
